<RunScreen>:
    FloatLayout:
        orientation: 'vertical'
        spacing:10
        canvas.before:
            Color:
                rgba: 0.498, 0.533, 0.553, 1
            Rectangle:
                pos: self.pos
                size: self.size

        Button:
            text: 'Go Back'
            size_hint: 1, None
            height: "40dp"
            on_press: root.go_back()